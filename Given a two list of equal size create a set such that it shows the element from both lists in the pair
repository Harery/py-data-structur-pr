list_one = [2, 3, 4, 5, 6, 7, 8]
list_two = [4, 9, 16, 25, 36, 49, 64]

list_three = set() # Answer1 #
list_four = set() # Answer2 #

#### Answer1 ####
list_three = set(zip(list_one,list_two))
print("the third list = ", list_three)

#### Answer4 ####
for x in range(len(list_one)):
    list_four.add((list_one[x], list_two[x]))
print("the fourth list = ",list_four)
