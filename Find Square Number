startv = int(input('eneter start number = '))
endv = int(input('enter end number = '))
stepv = int(input('enter your numbers steps = '))

print('Number  ','  Square')
print('--------------------')
for i in range(startv,endv,stepv):
    square = i**2
    print(i,'\t', '\t', '\t',square)
print('output done ')
