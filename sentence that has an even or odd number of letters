st = 'Print every word in this sentence that has an even or odd number of letters'

st = st.split( )
even_list=[]
odd_list=[]

for i in st:
    if len(i) %2 == 0:
        even_list.append(i)
    else:
        odd_list.append(i)
print("even_list = {}".format(even_list))
print("odd_list = {}".format(odd_list))
